{
    "name": "sadekbaroudi/gitorade",
    "description": "Git management and automation for software development organizations",
    "version": "0.0.1",
    "type": "library",
    "keywords": ["git", "management", "automation", "branch", "merge"],
    "homepage": "https://bitbucket.org/sadekbaroudi/gitorade",
    "license": "MIT",
    "authors": [
        {
            "name": "Sadek Baroudi",
            "email": "sadekbaroudi@gmail.com",
            "homepage": "https://bitbucket.org/sadekbaroudi",
            "role": "Architect, Developer"
        }
    ],
    "autoload": {
        "psr-0": { "Sadekbaroudi\\Gitorade\\": "" }
    },
    "require": {
        "php": ">=5.3.3",
        "symfony/console": ">=2.3.5",
        "symfony/yaml": ">=2.3.5",
        "symfony/dependency-injection": ">=2.3.5",
        "symfony/config": ">=2.3.5",
        "symfony/finder": ">=2.3.5",
        "symfony/event-dispatcher": ">=2.3.5",
        "cpliakas/git-wrapper": ">=1.1.1",
        "knplabs/github-api": "*",
        "sadekbaroudi/operation-state": "*@dev"
    },
    "require-dev": {
        "phpunit/phpunit": ">=3.7.0",
        "satooshi/php-coveralls": "dev-master"
    },
    "config": {
        "bin-dir": "bin"
    },
    "target-dir": "Sadekbaroudi/Gitorade",
    "minimum-stability": "stable"
}
